{
  "name": "subbe/waveapp",
  "description": "A Wrapper to consume WaveApp's GraphQL API",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Suhaib",
      "email": "mohamed.suhaib@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Subbe\\WaveApp\\": "src/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": ">=6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Subbe\\WaveApp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Subbe\\WaveApp\\WaveAppServiceProvider"
      ],
      "aliases": {
        "WaveApp": "Subbe\\WaveApp\\WaveApp"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "~3.0"
  }
}
